<% @items.each do |item| %>
    <div class="card" style="width: : 18rem;">
        <div class="card-body">
            <h5 class="card-title"><%= link_to item.name, item_path(item) %></h5>
            <h6 class="card-subtitle mb-2 text-muted">Description: <%= item.description %></h6>
            <h6 class="card-text"> Categories: <%= item.category_names.join(", ") %> </h6>
            <%= button_to "Add To Cart!", cart_add_to_path(cart_id: current_cart.id, item_id: item.id), method: :post, class: "btn btn-primary" %>
        </div>
    </div>
<% end %>